{
    "name": "mednetsportsapi",
    "type": "module",
    "version": "1.2.1.2",
    "description": "API for Mednetsports Mobile",
    "main": "dist",
    "scripts": {
        "clean": "rm -rf dist",
        "start": "npm run build && env-cmd --file .env node --experimental-specifier-resolution=node ./dist/api/index.js",
        "build": "npm run clean && tsc -p tsconfig.json && npm run copy-files",
        "lint": "eslint .",
        "copy-files": "copyfiles -u 1 src/json/**/* src/public/**/* dist",
        "dev": "npx tsx ./src/api/index.ts",
        "staging": "npm run build && env-cmd --file .env.staging node --experimental-specifier-resolution=node ./dist/api/index.js",
        "production": "npm run build && env-cmd --file .env.production node --experimental-specifier-resolution=node ./dist/api/index.js"
    },
    "eslintConfig": {
        "ecmaFeatures": {
            "modules": true
        }
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "@types/express-serve-static-core": "^4.17.35",
        "@types/swagger-jsdoc": "^6.0.1",
        "axios": "^1.3.4",
        "bcrypt": "^5.1.0",
        "body-parser": "^1.20.0",
        "cldr-data": "^36.0.1",
        "compression": "^1.7.4",
        "config": "^3.3.8",
        "cookie-parser": "^1.4.6",
        "cookie-session": "^1.4.0",
        "copyfiles": "^2.4.1",
        "cors": "^2.8.5",
        "cron": "^2.4.3",
        "dateformat": "^5.0.3",
        "dotenv": "^16.0.3",
        "ejs": "^3.1.8",
        "env-cmd": "^10.1.0",
        "express": "^4.18.2",
        "express-jwt": "^8.4.1",
        "express-validator": "^6.14.2",
        "firebase": "^9.23.0",
        "firebase-admin": "^11.9.0",
        "firebase-functions": "^4.4.1",
        "globalize": "^1.7.0",
        "ip": "^2.0.0",
        "jsonwebtoken": "^9.0.2",
        "mkdirp": "^3.0.1",
        "mongodb": "^5.7.0",
        "mongoose": "^6.8.2",
        "mongoose-autopopulate": "^0.16.1",
        "multer": "^1.4.5-lts.1",
        "nodemailer": "^6.8.0",
        "normalize": "^0.3.1",
        "normalize-path": "^3.0.0",
