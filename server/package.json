        "@types/compression": "^1.7.2",
        "@types/cookie-parser": "^1.4.3",
        "@types/cookie-session": "^2.0.44",
        "@types/cors": "^2.8.13",
        "@types/cron": "^2.4.0",
        "@types/dateformat": "^5.0.0",
        "@types/express": "^4.17.17",
        "@types/fs-extra": "^9.0.13",
        "@types/globalize": "^1.5.2",
        "@types/multer": "^1.4.7",
        "@types/node": "^18.16.3",
        "@types/nodemailer": "^6.4.7",
