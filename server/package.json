        "fs-extra": "^10.1.0",
        "nodemon": "^3.0.1",
        "npm-check-updates": "^16.6.2",
        "npm-run-all": "^4.1.5",
        "raw-loader": "^4.0.2",
        "rimraf": "^3.0.2",
        "shelljs": "^0.8.5",
        "ts-node": "^10.9.1",
